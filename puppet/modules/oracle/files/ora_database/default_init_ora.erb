###############################################################################
 
# install time)
 
db_name='<%= name %>'
memory_target=1G
processes = 150
audit_file_dest='<%= oracle_base -%>/admin/<%= name -%>/adump'
audit_trail = 'db'
db_block_size=8192
db_recovery_file_dest='<%= oracle_home -%>/dbs'
db_create_file_dest='<%= oracle_home -%>/dbs'
db_recovery_file_dest_size=2G
diagnostic_dest='<%= oracle_base -%>/admin/<%= name -%>/ddump'
dispatchers='(PROTOCOL=TCP) (SERVICE=<%= name -%>XDB)'
open_cursors=300 
remote_login_passwordfile='EXCLUSIVE'
